<div id="rsvp-form-container">
  <div class="rsvp-left"> </div>

  <div class="rsvp-right">
    <div class="rsvp-logo"></div>
    <div class="rsvp-stamp"></div>
    <%= form_tag('/guests/create', method: :post) do %>
      <%= render partial: '/guests/new' %>
      <div class="additional-guests"> </div>
      <a href="#" class="add-new-guest"> Add Another Guest </a>
      <div>
        <%= submit_tag 'Mail In RSVP', :class => "rsvpButton" %>
      </div>
    <% end %>
  </div>
</div>
